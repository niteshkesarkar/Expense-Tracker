<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="basic">
		<field
				name="title"
				type="text"
				label="Title"
				description="Enter title"
				required="true"
				/>
		<field
				name="description"
				type="textarea"
				label="Description"
				description="Enter description"
				required="true"
				/>
		<field
				name="created_date"
				type="calendar"
				label="Created Date"
				description="Enter created date"
				required="true"
				/>
		<field
				name="id"
				type="hidden"
				/>
		<field
				name="created_by"
				type="hidden"
				/>
	</fieldset>
</form>
